HISTORIAHUBButton.TextColor3 = Color3.fromRGB(255, 255, 255)
HISTORIAHUBButton.TextScaled = true
HISTORIAHUBButton.TextSize = 14.000
HISTORIAHUBButton.TextWrapped = true
HISTORIAHUBButton.MouseButton1Click:Connect(function()
	loadstring(game:HttpGet(('loadstring(game:HttpGet("https://raw.githubusercontent.com/Historia00012/HISTORIAHUB/main/BSS%20FREE"))()
		'),true))()
end)

