-- Gui to Lua

local InsertedObjects = Instance.new("ScreenGui")
local Gradient = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")
local UIGradient = Instance.new("UIGradient")
local UICorner = Instance.new("UICorner")
local HISTORIAHUBButton = Instance.new("TextButton")
local Shadow = Instance.new("Frame")
local TextLabel_2 = Instance.new("TextLabel")
local FunHubButton = Instance.new("TextButton")
local Shadow_2 = Instance.new("Frame")
local TextLabel_3 = Instance.new("TextLabel")
local AntiAFKButton = Instance.new("TextButton")
local Shadow_3 = Instance.new("Frame")
local TextLabel_4 = Instance.new("TextLabel")
local AltsegoDScriptButton = Instance.new("TextButton")
